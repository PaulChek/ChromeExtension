{
  "name": "Podcast Dark Mode",
  "author": "Paul Kravchenko",
  "description": "Add dark theme to google podcasts!",
  "permissions": [ "scripting", "activeTab", "storage" ],
  "version": "1.0.1",
  "manifest_version": 3,
  "icons": {
    "16": "images/16x16.png",
    "32": "images/32x32.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/16x16.png",
      "24": "images/24x24.png",
      "32": "images/32x32.png"
    }
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [ "https://podcasts.google.com/*", "https://podcasts.google.com/queue" ],
      "css": [ "css/Dark_Theme.css" ],
      "js": [ "js/content.js" ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  }
}